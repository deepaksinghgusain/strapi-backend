{
  "kind": "collectionType",
  "collectionName": "instructors",
  "info": {
    "singularName": "instructor",
    "pluralName": "instructors",
    "displayName": "Instructor",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string"
    },
    "bioData": {
      "type": "richtext"
    },
    "topics": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "email": {
      "type": "string",
      "required": true
    },
    "IsActive": {
      "type": "boolean",
      "default": true
    },
    "shortDesc": {
      "type": "text",
      "required": true
    },
    "courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course.course",
      "mappedBy": "instructors"
    }
  }
}
