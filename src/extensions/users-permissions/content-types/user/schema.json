{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string"
    },
    "address1": {
      "type": "string",
      "required": false
    },
    "address2": {
      "type": "string",
      "required": false
    },
    "phone": {
      "type": "string",
      "required": false
    },
    "PTIN": {
      "type": "string",
      "required": false
    },
    "companyName": {
      "type": "string",
      "required": false
    },
    "lastLoggedIn": {
      "type": "datetime"
    },
    "country": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "string",
      "required": true
    },
    "profileImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "coursePurchased": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-course.user-course",
      "mappedBy": "user"
    },
    "uuid": {
      "type": "uid",
      "private": true
    },
    "user_exams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-exam.user-exam",
      "mappedBy": "user"
    },
    "middleName": {
      "type": "string"
    }
  }
}
